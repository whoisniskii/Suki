{
  "name": "suki-rewrite",
  "description": "Suki code rewritten in Typescript",
  "version": "0.1",
  "private": true,
  "main": "./build/Initializer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Niskii3/Suki-Rewrite.git"
  },
  "bugs": {
    "url": "https://github.com/Niskii3/Suki-Rewrite/issues"
  },
  "scripts": {
    "build": "npx sucrase ./src -d ./build --esModuleInterop --transforms typescript,imports",
    "start": "node ./build/Initializer.js",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "types": "npx tsc --noEmit --incremental false"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "dayjs": "^1.11.2",
    "discord.js": "^14.0.0-dev.1652350334-4515a1e",
    "dotenv": "^16.0.1",
    "i18next": "^21.8.1",
    "i18next-fs-backend": "^1.1.4",
    "js-yaml": "^4.1.0",
    "mongoose": "^6.3.2",
    "undici": "^5.2.0",
    "vulkava": "^1.2.4"
  },
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "sucrase": "^3.21.0",
    "typescript": "^4.6.4"
  }
}
