{
  "name": "suki-rewrite",
  "description": "Suki code rewritten in Typescript",
  "version": "0.1",
  "private": true,
  "main": "./build/Initializer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Niskii3/Suki-Rewrite.git"
  },
  "bugs": {
    "url": "https://github.com/Niskii3/Suki-Rewrite/issues"
  },
  "scripts": {
    "build": "npx sucrase ./src -d ./build --esModuleInterop --transforms typescript,imports",
    "build:check": "yarn types:check && yarn build",
    "start": "node ./build/Initializer.js",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "lint:prettier": "npx prettier -w src",
    "types:check": "npx tsc --noEmit --incremental false"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "dayjs": "^1.11.2",
    "discord.js": "^14.0.0-dev.1652573522-7ce641d",
    "i18next": "^21.8.4",
    "i18next-fs-backend": "^1.1.4",
    "mongoose": "^6.3.4",
    "undici": "^5.3.0",
    "vulkava": "^1.2.4"
  },
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "sucrase": "^3.21.0",
    "typescript": "^4.7.2"
  }
}
