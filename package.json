{
  "name": "suki",
  "description": "Suki is a multipurpose Discord bot. Built with Node.js, TypeScript and discord.js.",
  "version": "1.0.0",
  "private": true,
  "main": "./build/Initializer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/whoisniskii/Suki.git"
  },
  "bugs": {
    "url": "https://github.com/whoisniskii/Suki/issues"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:check": "yarn types:check && yarn build",
    "start": "cd build && node Initializer.js",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "lint:prettier": "npx prettier -w src",
    "types:check": "npx tsc --noEmit --incremental false",
    "dev": "yarn build && yarn start"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "discord.js": "^14.0.0-dev.1654519696-fba9710",
    "i18next": "^21.8.8",
    "i18next-fs-backend": "^1.1.4",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/node": "^17.0.40",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  }
}
